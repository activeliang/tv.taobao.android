<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item>
        <shape>
            <stroke android:width="@dimen/dp_2" android:color="@color/colorWhite"/>
            <corners android:radius="@dimen/dp_1000"/>
        </shape>
    </item>
    <item android:left="@dimen/dp_2" android:top="@dimen/dp_2" android:right="@dimen/dp_2" android:bottom="@dimen/dp_2">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_1000"/>
            <stroke android:width="@dimen/dp_2" android:color="@color/alibc_transparent"/>
        </shape>
    </item>
    <item android:left="@dimen/dp_4" android:top="@dimen/dp_4" android:right="@dimen/dp_4" android:bottom="@dimen/dp_4">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_1000"/>
            <gradient android:startColor="#ff6000" android:endColor="#fe9000" android:angle="90"/>
        </shape>
    </item>
</layer-list>
