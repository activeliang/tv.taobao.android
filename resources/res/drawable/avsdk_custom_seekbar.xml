<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/avsdk_white_a"/>
            <size android:height="2dp"/>
            <corners android:radius="1dp"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="@color/avsdk_white_b"/>
                <size android:height="2dp"/>
                <corners android:radius="1dp"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/avsdk_progress"/>
                <size android:height="2dp"/>
                <corners android:radius="1dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
